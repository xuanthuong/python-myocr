doctype html
| {% load static %}
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    title #{title} | SI Automation
    meta(content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no', name='viewport')
    link(rel='stylesheet', href="{% static '/ocr/css/bootstrap.css' %}")
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css')
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css')
    link(rel='stylesheet', href="{% static '/ocr/css/AdminLTE.min.css' %}")
    link(rel='stylesheet', href="{% static '/ocr/css/skins/skin-blue.min.css' %}")
    // iCheck
    link(rel='stylesheet', href="{% static '/ocr/plugins/iCheck/flat/blue.css' %}")
    // Morris chart
    link(rel='stylesheet', href="{% static '/ocr/plugins/morris/morris.css' %}")
    // jvectormap
    link(rel='stylesheet', href="{% static '/ocr/plugins/jvectormap/jquery-jvectormap-1.2.2.css' %}")
    // Date Picker
    link(rel='stylesheet', href="{% static '/ocr/plugins/datepicker/datepicker3.css' %}")
    // Daterange picker
    link(rel='stylesheet', href="{% static '/ocr/plugins/daterangepicker/daterangepicker.css' %}")
    // bootstrap wysihtml5 - text editor
    link(rel='stylesheet', href="{% static '/ocr/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css' %}")
    block page_css
    //
    //if lt IE 9
    //script(src='https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js')
    //script(src='https://oss.maxcdn.com/respond/1.4.2/respond.min.js')
  body.hold-transition.skin-blue.sidebar-mini.fixed.sidebar-collapse
    .wrapper
        include ocr/partials/header
        include ocr/partials/main-sidebar
        .content-wrapper
          block content
        include ocr/partials/footer
        include ocr/partials/control-sidebar
    // ./wrapper
    // jQuery 2.2.3
    script(src="{% static '/ocr/plugins/jQuery/jquery-2.2.3.min.js'%}")
    // jQuery UI 1.11.4
    script(src='https://code.jquery.com/ui/1.11.4/jquery-ui.min.js')
    // Resolve conflict in jQuery UI tooltip with Bootstrap tooltip
    script.
      $.widget.bridge('uibutton', $.ui.button);
    // Bootstrap 3.3.6
    script(src="{% static '/ocr/js/bootstrap.min.js'%}")
    // Morris.js charts
    script(src='https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js')
    script(src="{% static '/ocr/plugins/morris/morris.min.js'%}")
    // Sparkline
    script(src="{% static '/ocr/plugins/sparkline/jquery.sparkline.min.js'%}")
    // jvectormap
    script(src="{% static '/ocr/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'%}")
    script(src="{% static '/ocr/plugins/jvectormap/jquery-jvectormap-world-mill-en.js'%}")
    // jQuery Knob Chart
    script(src="{% static '/ocr/plugins/knob/jquery.knob.js'%}")
    // daterangepicker
    script(src='https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js')
    script(src="{% static '/ocr/plugins/daterangepicker/daterangepicker.js'%}")
    // datepicker
    script(src="{% static '/ocr/plugins/datepicker/bootstrap-datepicker.js'%}")
    // Bootstrap WYSIHTML5
    script(src="{% static '/ocr/plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js'%}")
    // Slimscroll
    script(src="{% static '/ocr/plugins/slimScroll/jquery.slimscroll.min.js'%}")
    // FastClick
    script(src="{% static '/ocr/plugins/fastclick/fastclick.js'%}")
    // AdminLTE App
    script(src="{% static '/ocr/js/admin.js'%}")
    script(src="{% static '/ocr/plugins/tesseract/tesseract.js'%}")
    script(src='https://cdnjs.cloudflare.com/ajax/libs/bootbox.js/4.4.0/bootbox.min.js')
    block page_script


                    
            
